    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/myaddress"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/myadd"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_car" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@+id/myadd"
                android:text="我的车辆"
                android:textSize="15dp" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>



        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/Background" />


        <RelativeLayout
            android:id="@+id/order_history"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/helpss"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_history" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@+id/helpss"
                android:text="停车记录"
                android:textSize="15dp" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/Background" />

        <RelativeLayout
            android:id="@+id/modify_tation"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/helpsas"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_adhome" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@+id/helpsas"
                android:text="我的地址"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/station_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:layout_toLeftOf="@+id/ig"
                android:text=""
                android:textSize="18dp" />

            <ImageView
                android:id="@+id/ig"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="18dp"
            android:background="@color/Background" />

        <RelativeLayout
            android:id="@+id/about_app"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/helps"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_about" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@+id/helps"
                android:text="关于"
                android:textSize="15dp" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/Background" />

        <RelativeLayout
            android:id="@+id/advice"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/advise"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_advice" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@+id/advise"
                android:text="意见反馈"
                android:textSize="15dp" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/Background" />

        <RelativeLayout
            android:id="@+id/setting"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/common_user_select"
            android:clickable="true"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/set"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_margin="8dp"
                android:src="@mipmap/common_fragment_setting" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_toEndOf="@+id/set"
                android:layout_toRightOf="@+id/set"
                android:text="设置"
                android:textSize="15dp" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@mipmap/common_fragment_enter" />
        </RelativeLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Background" />
</LinearLayout>